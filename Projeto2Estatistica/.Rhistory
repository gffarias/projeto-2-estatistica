library(readr)
Detalhes_de_Albuns <- read_csv("Detalhes de Albuns.csv")
View(Detalhes_de_Albuns)
dados = data.frame(Detalhes_de_Albuns)
print(dados)
mean(dados$Qnt..de.Albuns.Vendidos)
sd(dados$Qnt..de.Albuns.Vendidos)
statMode <- function (y) {
freq <- table(y)
maxFreq <- max(freq)
if (all(freq == maxFreq))
return ("Amodal")
else
return (names(freq)[freq == maxFreq])
}
statMode(dados$Qnt..de.Albuns.Vendidos)
